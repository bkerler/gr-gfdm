    type: complex
    vlen: '1'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [56, 300]
    rotation: 0
    state: enabled
- name: ref_preamble
  id: parameter
  parameters:
    alias: ''
    comment: ''
    hide: none
    label: Preamble
    short_id: ''
    type: ''
    value: np.ones(128)
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [376, 20]
    rotation: 0
    state: enabled
- name: sc_high_thr
  id: parameter
  parameters:
    alias: ''
    comment: ''
    hide: none
    label: SC Lower Threshold
    short_id: ''
    type: eng_float
    value: '.7'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [1008, 20]
    rotation: 0
    state: enabled
- name: sc_low_thr
  id: parameter
  parameters:
    alias: ''
    comment: ''
    hide: none
    label: SC Lower Threshold
    short_id: ''
    type: eng_float
    value: '.6'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [832, 20]
    rotation: 0
    state: enabled
- name: xcorr_comp_freq
  id: parameter
  parameters:
    alias: ''
    comment: ''
    hide: none
    label: Xcorr compensate freq.
    short_id: ''
    type: ''
    value: 'False'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [1368, 20]
    rotation: 0
    state: enabled
- name: xcorr_det_thr
  id: parameter
  parameters:
    alias: ''
    comment: ''
    hide: none
    label: XCorr Detection Threshold
    short_id: ''
    type: eng_float
    value: '100.'
  states:
    bus_sink: false
    bus_source: false
    bus_structure: null
    coordinate: [1184, 20]
    rotation: 0
    state: enabled

connections:
- [gfdm_extract_burst_cc_0, '0', frames, '0']
- [hier_multicarrier_sync_0, '0', frames_0, '0']
- [hier_multicarrier_sync_0, '0', gfdm_extract_burst_cc_0, '0']
- [hier_multicarrier_sync_0, '1', frames_0_0, '0']
- [pad_source_0, '0', hier_multicarrier_sync_0, '0']

metadata:
  file_format: 1
