<block>
  <name>GFDM Framer</name>
  <key>gfdm_framer_cc</key>
  <category>GFDM</category>
  <import>import gfdm</import>
  <make>gfdm.framer_cc($nsubcarrier, $ntimeslots, $sync, $sync_symbols, $len_tag_key)</make>
  <param>
    <name>Nsubcarrier</name>
    <key>nsubcarrier</key>
    <value>16</value>
    <type>int</type>
  </param>
  <param>
    <name>Ntimeslots</name>
    <key>ntimeslots</key>
    <value>16</value>
    <type>int</type>
  </param>
  <param>
    <name>Sync</name>
    <key>sync</key>
    <value>false</value>
    <type>bool</type>
  </param>
  <param>
    <name>Sync_symbols</name>
    <key>sync_symbols</key>
    <value>[]</value>
    <type>complex_vector</type>
  </param>
  <param>
    <name>Len_tag_key</name>
    <key>len_tag_key</key>
    <value> "frame_len"</value>
    <type>string</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
