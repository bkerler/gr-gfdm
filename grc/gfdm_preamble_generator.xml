<?xml version="1.0"?>
<block>
  <name>GFDM Preamble Generator</name>
  <key>gfdm_preamble_generator</key>
  <category>GFDM</category>
  <import>import gfdm</import>
  <make>gfdm.preamble_generator($nsubcarrier, $filter_alpha, $sync_fft_len)</make>
  <param>
	<name>Nsubcarrier</name>
	<key>nsubcarrier</key>
	<value>16</value>
	<type>int</type>
  </param>
  <param>
    	<name>Filter alpha</name>
    	<key>filter_alpha</key>
    	<value>0.35</value>
    	<type>real</type>
  </param>
  <param>
	  <name>Sync FFT Len</name>
	  <key>sync_fft_len</key>
	  <value>32</value>
	  <type>int</type>
  </param>
</block>
