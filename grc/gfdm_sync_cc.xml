<block>
  <name>Sync cc</name>
  <key>gfdm_sync_cc</key>
  <category>GFDM</category>
  <import>import gfdm</import>
  <make>gfdm.sync_cc($sync_fft_len, $cp_length, $fft_len)</make>
  <param>
    <name>Sync_fft_len</name>
    <key>sync_fft_len</key>
    <value>32</value>
    <type>int</type>
  </param>
  <param>
    <name>Cp_length</name>
    <key>cp_length</key>
    <value>0</value>
    <type>int</type>
  </param>
  <param>
    <name>Fft_len</name>
    <key>fft_len</key>
    <value>256</value>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
  <source>
    <name>corr_out</name>
    <type>complex</type>
    <optional>1</optional>
  </source>
</block>
