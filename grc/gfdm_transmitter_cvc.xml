<block>
  <name>Transmitter cvc</name>
  <key>gfdm_transmitter_cc</key>
  <category>GFDM</category>
  <import>import gfdm</import>
  <make>gfdm.transmitter_cc($nsubcarrier, $ntimeslots, $filter_width, $filter_alpha)</make>
  <param>
    <name>Nsubcarrier</name>
    <key>nsubcarrier</key>
    <value>64</value>
    <type>int</type>
  </param>
  <param>
    <name>Ntimeslots</name>
    <key>ntimeslots</key>
    <value>16</value>
    <type>int</type>
  </param>
  <param>
    <name>Filter_width</name>
    <key>filter_width</key>
    <value>2</value>
    <type>int</type>
  </param>
  <param>
    <name>Filter_alpha</name>
    <key>filter_alpha</key>
    <value>0.35</value>
    <type>real</type>
  </param>
  <param>
    <name>Num_inputs</name>
    <key>num_inputs</key>
    <value>1</value>
    <type>int</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
    <nports>$num_inputs</nports>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>$nsubcarrier * ntimeslots</vlen>
  </source>
</block>
